<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gglee.sample.exproc.mapper.TBoardMapper">
    <select id="selectBoardList" resultType="TBoardVO">
        SELECT
            board_no,
            board_title,
            board_content,
            board_writer,
            reg_date,
            update_date
        FROM
            test_board
    </select>
</mapper>